        OneSignal.push(function () {
            OneSignal.init({
                appId: "571b8baa-797b-41e3-ac8d-f4c1bb196d29",
                autoRegister: true,
                notifyButton: {
                    enable: false,
                },
            });
        });
    </script>

    <!-- end wrapper -->
    <!-- Footer Scripts -->
    <!-- JS | Custom script for all pages -->

